            express1.append(str(c))
            express1.append(str(x))
            print("--------------")
else:
    print("\nTruth Table for Expression1: ")
    print("------------------")
    print(" | A | B | C | X |")
    print("------------------")
    express1 = []
    for a in range(0,2):
        for b in range(0,2):
            for c in range(0,2):
                x = eval(expression1)
                print(" | " + str(a) + " | " + str(b) +" | " + str(c) + " | " + str(x) + " | ")
                express1.append(str(a))
                express1.append(str(b))
                express1.append(str(c))
                express1.append(str(x))
                print("------------------")

