expression1 = input("Please enter the first expression: ")
expression2 = input("Please enter the second expression: ")
print("\nBoolean Expression: " + expression1.upper())
expression1 = expression1 = expression1.lower()
expression1 = expression1.replace("and", "&")
expression1 = expression1.replace("or", "|")
expression1 = expression1.replace("not", "~")

if "c" not in expression1:
    print("\nTruth Table for Expression1: ")
    print("--------------")
    print(" | A | B | X |")
    print("--------------")
    express1 = []
    for a in range(0,2):
        for b in range(0,2):
            x = eval(expression1)
            print(" | " + str(a) + " | " + str(b) + " | " + str(x) + " | ")
            express1.append(str(a))
            express1.append(str(b))
            express1.append(str(x))
            print("--------------")
elif "b" not in expression1:
    print("\nTruth Table for Expression1: ")
    print("--------------")
    print(" | A | C | X |")
    print("--------------")
    express1 = []
    for a in range(0,2):
        for c in range(0,2):
            x = eval(expression1)
            print(" | " + str(a) + " | " + str(c) + " | " + str(x) + " | ")
            express1.append(str(a))
            express1.append(str(c))
            express1.append(str(x))
            print("--------------")
elif "a" not in expression1:
    print("\nTruth Table for Expression1: ")
    print("--------------")
    print(" | B | C | X |")
